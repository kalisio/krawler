    "dockerode": "^2.5.5",
    "envsub": "^4.0.7",
    "fast-csv": "^4.3.6",
    "feathers-hooks-common": "^3.0.0",
    "fs-blob-store": "^5.2.1",
    "fs-extra": "^8.1.0",
    "helmet": "^3.5.0",
    "js-yaml": "^3.13.1",
    "json2csv": "^3.11.4",
    "kue": "^0.11.6",
    "lodash": "^4.17.15",
