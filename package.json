    "reproject": "^1.2.1",
    "request": "^2.88.0",
    "s3-blob-store": "^2.1.0",
    "sift": "^6.0.0",
    "tar": "^4.4.4",
    "unzipper": "^0.9.5",
    "uuid": "^3.2.1",
    "winston": "^3.7.2",
    "xml2js": "^0.4.23",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "c8": "^7.11.2",
    "chai": "^4.2.0",
    "chai-lint": "^0.1.1",
    "loader-runner": "^3.0.0",
    "mocha": "^9.0.0",
    "nock": "^12.0.0",
    "shx": "^0.3.2",
    "standard": "^17.0.0",
    "stylus-loader": "^3.0.2",
